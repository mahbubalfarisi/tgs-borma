function update_date_form(field,date)
{d=date.getDate();m=date.getMonth()+1;y=date.getFullYear();$('input[name='+field+'_yy]').val(y);$('select[name='+field+'_mm]').val(m);$('select[name='+field+'_dd]').val(d);}

